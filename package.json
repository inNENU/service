{
  "name": "innenu-service",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "bundle:encrypt": "rollup -c scripts/encrypt.ts --configPlugin esbuild",
    "debug": "cross-env NODE_ENV=development pnpm build && cross-env NODE_ENV=development node dist/index.js --expose-gc",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "lint": "eslint --fix . && prettier --check --write .",
    "lint:check": "eslint . && prettier --check .",
    "prepare": "husky"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@alicloud/alinlp20200629": "3.1.0",
    "@alicloud/openapi-client": "0.4.15",
    "@mptool/net": "0.11.0",
    "@mptool/parser": "0.11.1",
    "body-parser": "2.2.0",
    "cheerio": "1.1.0",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "crypto-js": "4.2.0",
    "dotenv": "17.0.1",
    "express": "5.1.0",
    "express-rate-limit": "7.5.1",
    "iconv-lite": "0.6.3",
    "js-sha1": "0.7.0",
    "morgan": "1.10.0",
    "mysql2": "3.14.1",
    "picocolors": "1.1.1",
    "qrcode": "1.5.4",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@types/body-parser": "1.19.6",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.3",
    "@types/express-serve-static-core": "5.0.6",
    "@types/morgan": "1.9.10",
    "@types/node": "24.0.10",
    "@types/qrcode": "1.5.5",
    "@types/qs": "6.14.0",
    "@types/uuid": "10.0.0",
    "cross-env": "7.0.3",
    "domhandler": "^5.0.3",
    "eslint": "9.30.1",
    "eslint-config-mister-hope": "0.9.6",
    "husky": "9.1.7",
    "nano-staged": "0.8.0",
    "prettier": "3.6.2",
    "rollup": "4.44.2",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-esbuild": "6.2.1",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  },
  "engines": {
    "node": ">= 20"
  }
}

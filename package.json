{
  "name": "innenu-service",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innenu/service.git"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "bundle:encrypt": "rollup -c scripts/encrypt.ts --configPlugin esbuild",
    "debug": "cross-env NODE_ENV=development pnpm build && cross-env NODE_ENV=development node dist/index.js --expose-gc",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "lint": "eslint --fix . && prettier --check --write .",
    "lint:check": "eslint . && prettier --check .",
    "prepare": "husky"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@alicloud/alinlp20200629": "3.1.0",
    "@alicloud/openapi-client": "0.4.15",
    "@mptool/net": "0.12.0",
    "@mptool/parser": "0.12.0",
    "cheerio": "1.1.2",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.6",
    "crypto-js": "4.2.0",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "express-rate-limit": "8.2.1",
    "iconv-lite": "0.7.0",
    "js-sha1": "0.7.0",
    "morgan": "1.10.1",
    "mysql2": "3.16.0",
    "picocolors": "1.1.1",
    "qrcode": "1.5.4",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@rollup/plugin-alias": "6.0.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@types/compression": "1.8.1",
    "@types/cookie-parser": "1.4.10",
    "@types/cors": "2.8.19",
    "@types/crypto-js": "4.2.2",
    "@types/express": "5.0.6",
    "@types/express-serve-static-core": "5.1.0",
    "@types/morgan": "1.9.10",
    "@types/node": "24.10.1",
    "@types/qrcode": "1.5.6",
    "@types/qs": "6.14.0",
    "cross-env": "10.1.0",
    "domhandler": "^5.0.3",
    "eslint": "9.39.1",
    "eslint-config-mister-hope": "0.9.6",
    "husky": "9.1.7",
    "nano-staged": "0.9.0",
    "prettier": "3.7.3",
    "rollup": "4.53.3",
    "rollup-plugin-dts": "6.3.0",
    "rollup-plugin-esbuild": "6.2.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">= 20.19"
  }
}
